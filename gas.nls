to gas-expand  
  
 let g random-float 1.00000001
  let probability 0.5
 
  if g <= probability [
    
    let nb one-of neighbors
    while [[patch-state] of nb = "WALL"] [
      set nb one-of neighbors   
    ]
    
    if count gases in-radius 5 > 2 [
      
      face nb
      forward 1 
      
    ] 
   
        
  ]
  
end
